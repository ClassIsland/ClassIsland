<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="RainyColorBrush" Color="Blue"/>
    <DropShadowEffect x:Key="LuminousClass"
                      BlurRadius="6"
                      OffsetX="0" OffsetY="0"
                      Color="#FF0" />

    <!-- AQI 颜色 -->
    <SolidColorBrush x:Key="AqiLevel1Brush">ForestGreen</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel2Brush">#cc0</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel3Brush">DarkOrange</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel4Brush">IndianRed</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel5Brush">DarkRed</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel6Brush">Purple</SolidColorBrush>

    <!-- 自定义背景色 -->
    <SolidColorBrush x:Key="CustomizedAccentBarBackground1Brush" Color="#FFDDE6F0" />
    
    <SolidColorBrush x:Key="TextFillColorInformativeSecondaryBrush" Color="#c0000000" />
</ResourceDictionary>