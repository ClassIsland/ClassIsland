<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="RainyColorBrush" Color="DeepSkyBlue"/>
    <DropShadowEffect x:Key="LuminousClass"
                      BlurRadius="2"
                      OffsetX="0" OffsetY="0"
                      Color="#FF0" />

    <!-- AQI 颜色 -->
    <SolidColorBrush x:Key="AqiLevel1Brush">MediumSeaGreen</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel2Brush">Yellow</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel3Brush">Orange</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel4Brush">IndianRed</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel5Brush">DarkRed</SolidColorBrush>
    <SolidColorBrush x:Key="AqiLevel6Brush">MediumPurple</SolidColorBrush>

    <!-- 自定义背景色 -->
    <SolidColorBrush x:Key="CustomizedAccentBarBackground1Brush" Color="#26374C" />
    
    <SolidColorBrush x:Key="TextFillColorInformativeSecondaryBrush" Color="#daffffff" />
</ResourceDictionary>